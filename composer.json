{
    "name": "cejobelo/hamtaro-starter",
    "description": "Start your Hamtaro application with hamtaro-starter.",
    "type": "project",
    "license": "MIT",
    "keywords": ["hamtaro", "hamtaro-starter"],

    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },

    "require": {
        "cejobelo/hamtaro": "v1.1.0-alpha",
        "michelf/php-markdown": "^1.9"
    },

    "authors": [
        {
            "name": "Phil'dy Jocelyn Belcou",
            "email": "pj.belcou@gmail.com",
            "homepage": "https://github.com/cejobelo",
            "role": "Developer"
        }
    ],

    "scripts": {
        "ajax": "Hamtaro\\Command\\CreateAjaxRequest::run",
        "component": "Hamtaro\\Command\\CreateComponent::run",
        "form": "Hamtaro\\Command\\CreateForm::run",
        "modal": "Hamtaro\\Command\\CreateModal::run",
        "page": "Hamtaro\\Command\\CreatePage::run",
        "event": "Hamtaro\\Command\\CreateEvent::run"
    }
}
